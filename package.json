    "@enhanced-dom/react": "^0.0.6",
    "@fontsource/montserrat": "^4.5.10",
    "@theme-ui/typography": "^0.14.5",
    "deepmerge": "^4.2.2",
    "lodash.pick": "^4.4.0",
    "react-card-flip": "^1.1.1",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@enhanced-dom/lint": "^0.0.7",
    "@enhanced-dom/webpack": "^0.0.7",
